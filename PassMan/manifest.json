{
  "name": "PassMan", 
  "version": "1.12",
  "manifest_version": 2,
  "description" : "PBKDF2 ve SHA-256 özetleme algoritması ile tek bir ana paroladan güçlü parolalar oluşturur.",
  "icons": {
    "128": "icon_128.png",
    "64": "icon_64.png"
  },
  "permissions": [
    "activeTab"
  ],
  "browser_action": {
      "default_title": "PassMan",
      "default_icon": "icon.png",
      "default_popup": "popup.html"
  },
  "content_security_policy": "default-src 'self'"
}
